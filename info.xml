<?xml version="1.0"?>
<extension key="com.joineryhq.autoincfield" type="module">
  <file>autoincfield</file>
  <name>Autoincrement Field</name>
  <description>Supports creation of a read-only custom field which is automatically incremented for each new entity (Individual, Organization, etc.)</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.autoincfield/</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.autoincfield/</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.autoincfield/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2020-08-21</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed by Joinery for the CiviCRM community.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Autoincfield</namespace>
  </civix>
</extension>
